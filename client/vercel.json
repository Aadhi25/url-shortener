{
  "rewrites": [
    {
      "source": "/dashboard/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/auth/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/((?!favicon.ico|.*\\..*).*)",
      "destination": "https://api.shrinkit.co.in/:1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
