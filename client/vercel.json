{
  "rewrites": [
    {
      "source": "/r/:shorturl",
      "destination": "https://api.shrinkit.co.in/r/:shorturl"
    },
    {
      "source": "/r/:shorturl/(.*)",
      "destination": "https://api.shrinkit.co.in/r/:shorturl"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
